\begin_inset Formula \begin{eqnarray}
\nabla^{2}\vt E & = & \mu\sigma\frac{\partial\vt E}{\partial t}+\mu\epsilon\frac{\partial^{2}\vt E}{\partial t^{2}}\\
\nabla^{2}\vt H & = & \mu\sigma\frac{\partial\vt H}{\partial t}+\mu\epsilon\frac{\partial^{2}\vt H}{\partial t^{2}}\end{eqnarray}

\end_inset


\end_layout

\begin_layout Itemize
no domínio da freqüência (campos harmônicos):
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{eqnarray}
\nabla^{2}\vti[s]E-\gamma^{2}\vti[s]E & = & 0\\
\nabla^{2}\vti[s]H-\gamma^{2}\vti[s]H & = & 0\end{eqnarray}

\end_inset


\end_layout

\begin_layout Itemize
solução das equações de onda de Helmholtz, para caso geral:
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{eqnarray}
\vt E(z,t) & = & E_{0}^{+}e^{-\alpha z}\cos(\omega t-\beta z)\ax\\
 &  & +E_{0}^{-}e^{\alpha z}\cos(\omega t+\beta z)\ax\\
\vt H(z,t) & = & H_{0}^{+}e^{-\alpha z}\cos(\omega t-\beta z)\ay\\
 &  & +H_{0}^{-}e^{\alpha z}\cos(\omega t+\beta z)\ay\end{eqnarray}

\end_inset


\end_layout

\begin_layout Subsection
Relação entre Ondas Propagantes (em fasores)
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{eqnarray}
\vti[s]H & = & \pv{\frac{1}{\eta}\arh}{\vti[s]E}\\
\vti[s]E & = & \pv{-\eta\arh}{\vti[s]H}\end{eqnarray}

\end_inset


\end_layout

\begin_layout Subsection
Constantes e Impedância Intrínseca
\end_layout

\begin_layout Subsubsection
Caso geral
\end_layout

\begin_layout Itemize
Constante de propagação:
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{equation}
\gamma=\sqrt{j\omega\mu(\sigma+j\omega\epsilon)}=\alpha+j\beta\end{equation}

\end_inset


\end_layout

\begin_layout Itemize
constante de atenuação:
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{equation}
\alpha=\omega\sqrt{\frac{\mu\epsilon}{2}\left(\sqrt{1+\left(\frac{\sigma}{\omega\epsilon}\right){}^{2}}-1\right)}\end{equation}

\end_inset


\end_layout

\begin_layout Itemize
constante de fase:
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{equation}
\beta=\omega\sqrt{\frac{\mu\epsilon}{2}\left(\sqrt{1+\left(\frac{\sigma}{\omega\epsilon}\right)^{2}}+1\right)}\end{equation}

\end_inset


\end_layout

\begin_layout Itemize
impedância intrínseca:
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{equation}
\eta=\sqrt{\frac{j\omega\mu}{\sigma+j\omega\epsilon}}\end{equation}

\end_inset


\end_layout

\begin_layout Subsubsection
Dielétricos com baixas perdas (
\begin_inset Formula $\frac{\sigma}{\omega\epsilon}\ll1$
\end_inset

)
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{eqnarray}
\alpha & \approx & \frac{\sigma}{2}\sqrt{\frac{\mu}{\epsilon}}\\
\beta & \approx & \omega\sqrt{\mu\epsilon}\\
\eta & \approx & \sqrt{\frac{\mu}{\epsilon}}\end{eqnarray}

\end_inset


\end_layout

\begin_layout Subsubsection
Bons condutores (
\begin_inset Formula $\frac{\sigma}{\omega\epsilon}\gg1$
\end_inset

)
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{eqnarray}
\alpha & \approx & \sqrt{\pi f\mu\sigma}\\
\beta & \approx & \sqrt{\pi f\mu\sigma}\\
\eta & \approx & \sqrt{\frac{\omega\mu}{\sigma}}e^{j45\degree}\approx\sqrt{2}\frac{\alpha}{\sigma}e^{j45\degree}\end{eqnarray}

\end_inset


\end_layout

\begin_layout Subsection
Velocidade de Propagação
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{equation}
u_{p}=\frac{\omega}{\beta}\end{equation}

\end_inset


\end_layout

\begin_layout Subsection
Comprimento de Onda
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{equation}
\lambda=\frac{u_{p}}{f}\end{equation}

\end_inset


\end_layout

