\end_layout

\begin_layout Itemize
coordenadas esféricas
\end_layout

\begin_layout Standard
\begin_inset Formula \[
\dvg{\vt A}=\frac{1}{r^{2}}\frac{\partial}{\partial r}(r^{2}A_{r})+\frac{1}{r\sin\theta}\frac{\partial}{\partial\theta}(A_{\theta}\sin\theta)+\frac{1}{r\sin\theta}\frac{\partial A_{\phi}}{\partial_{\phi}}\]

\end_inset


\end_layout

\end_deeper
\begin_layout Itemize
gradiente:
\end_layout

\begin_deeper
\begin_layout Itemize
coordenadas cartesianas
\end_layout

\begin_layout Standard
