\begin_layout Section
Eletrostática
\end_layout

\begin_layout Subsection
Lei de Coulomb
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{equation}
\vec{F}_{12}=\frac{Q_{1}Q_{2}}{4\pi\epsilon R_{12}^{2}}\aud\end{equation}

\end_inset


\end_layout

\begin_layout Subsection
Campo Elétrico
\end_layout

\begin_layout Itemize
Caso geral:
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{align}
\vec{E}_{1} & =\frac{\vec{F}_{12}}{Q_{2}}\\
\vec{E} & =\frac{Q}{4\pi\epsilon R^{2}}\aR\end{align}

\end_inset


\end_layout

\begin_layout Itemize
de distribuição contínua de cargas:
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{equation}
\vt E=\int\frac{dQ}{4\pi\epsilon R^{2}}\aR\end{equation}

\end_inset


\end_layout

\begin_layout Itemize
de uma carga pontual na origem:
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{equation}
\vt E=\frac{Q}{4\pi\epsilon r^{2}}\ar\end{equation}

\end_inset


\end_layout

\begin_layout Itemize
de uma linha infinita em 
\begin_inset Formula $z$
\end_inset

 carregada com 
\begin_inset Formula $\rho_{L}$
\end_inset

:
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{equation}
\vt E=\frac{\rho_{L}}{2\pi\epsilon\rho}\arh\end{equation}

\end_inset


\end_layout

\begin_layout Itemize
de uma lâmina infinita carregada com 
\begin_inset Formula $\rho_{S}$
\end_inset

:
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{equation}
\vt E=\frac{\rho_{S}}{2\epsilon}\aN\end{equation}

\end_inset


\end_layout

\begin_layout Subsection
Densidade de Fluxo Elétrico
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{equation}
\vt D=\epsilon\vt E\end{equation}

\end_inset


\end_layout

\begin_layout Subsection
Fluxo Elétrico
\end_layout

\begin_layout Itemize
Que atravessa uma superfície:
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{equation}
\Psi=\int\pe{\vt D}{\dS}\end{equation}

\end_inset


\end_layout

\begin_layout Itemize
que atravessa uma superfície fechada:
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{equation}
\Psi=\oint\pe{\vt D}{\dS}\end{equation}

\end_inset


\end_layout

\begin_layout Subsection
Teorema da Divergência
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{equation}
\oint\pe{\vt D}{\dS}=\int\pe{\nabla}{\vt D\dv}\end{equation}

\end_inset


\end_layout

\begin_layout Subsection
Lei de Gauss
\begin_inset CommandInset label
LatexCommand label
name "sub:Lei-de-Gauss"

\end_inset


\end_layout

\begin_layout Itemize
Forma integral:
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{eqnarray}
\oint\pe{\vt D}{\dS} & = & Q_{env}=\Psi_{resultante}\\
\oint\pe{\vt D}{\dS} & = & \int\rhv\dv\end{eqnarray}

\end_inset


\end_layout

\begin_layout Itemize
forma diferencial:
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{equation}
\pe{\nabla}{\vt D}=\rhv\end{equation}

\end_inset


\end_layout

\begin_layout Subsection
Pontencial Elétrico
\end_layout

\begin_layout Itemize
Diferença de potencial elétrico:
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{equation}
V_{ba}=-\int_{a}^{b}\pe{\vt E}{\dL}=V_{b}-V_{a}\end{equation}

\end_inset


\end_layout

\begin_layout Itemize
potencial com referência no infinito:
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{equation}
V=\int\frac{\dQ}{4\pi\epsilon r}\end{equation}

\end_inset


\end_layout

\begin_layout Itemize
campo elétrico a partir de função potencial:
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{equation}
\vt E=-\nabla V\end{equation}

\end_inset


\end_layout

\begin_layout Subsection
Densidade de Corrente
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{equation}
\vt J=\sigma\vt E\end{equation}

\end_inset


\end_layout

\begin_layout Subsection
Corrente
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{equation}
I=\int\pe{\vt J}{\dS}\end{equation}

\end_inset


\end_layout

\begin_layout Subsection
Resistência
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{equation}
R=\frac{-\int\pe{\vt E}{\dL}}{\int\sigma\pe{\vt E}{\dS}}\end{equation}

\end_inset


\end_layout

\begin_layout Subsection
Lei de Joule
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{equation}
P=\int\pe{\vt E}{\vt J}\dv\end{equation}

\end_inset


\end_layout

\begin_layout Subsection
Condições de Fronteira
\end_layout

\begin_layout Itemize
Entre par de dielétricos:
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{eqnarray}
\vti[T1]E & = & \vti[T2]E\\
\pe{\aind{21}}{(\vti[1]D-\vti[2]D)} & = & \rhs\end{eqnarray}

\end_inset


\end_layout

\begin_layout Itemize
entre par de dielétricos, se 
\begin_inset Formula $\rhs=0$
\end_inset

:
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{eqnarray}
\vti[T1]E & = & \vti[T2]E\\
\vti[N1]D & = & \vti[N2]D\end{eqnarray}

\end_inset


\end_layout

\begin_layout Itemize
entre condutor e dielétrico:
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{eqnarray}
\vti[T]E & = & 0\\
\vti[N]D & = & \rhs\end{eqnarray}

\end_inset


\end_layout

\begin_layout Subsection
Equação de Poisson
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{equation}
\nabla^{2}V=-\frac{\rhv}{\epsilon}\end{equation}

\end_inset


\end_layout

\begin_layout Subsection
Equação de Laplace
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{equation}
\nabla^{2}V=0\end{equation}

\end_inset


\end_layout

\begin_layout Subsection
Capacitância
\end_layout

\begin_layout Itemize
Definição geral:
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{equation}
C=\frac{\dQ}{\dV}\end{equation}

\end_inset


\end_layout

\begin_layout Itemize
para capacitor de placas paralelas, desprezando-se efeitos de borda:
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{equation}
C=\frac{\epsilon S}{d}\end{equation}

\end_inset


\end_layout

\begin_layout Subsection
Energia Potencial Eletrostática
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{equation}
W_{E}=\frac{1}{2}\int\pe{\vt D}{\vt E}\dv=\frac{1}{2}\int\epsilon E^{2}\dv=\frac{1}{2}CV^{2}\end{equation}

\end_inset


\end_layout

