\end_inset


\end_layout

\begin_layout Itemize
coordenadas cilíndricas
\end_layout

\begin_layout Standard
\begin_inset Formula \[
\nabla V=\frac{\partial V}{\partial\rho}\arh+\frac{1}{\rho}\frac{\partial V}{\partial\phi}\aph+\frac{\partial V}{\delz}\az\]

\end_inset


\end_layout

\begin_layout Itemize
coordenadas esféricas
\end_layout

\begin_layout Standard
\begin_inset Formula \[
\nabla V=\frac{\partial V}{\partial r}\ar+\frac{1}{r}\frac{\partial V}{\partial\theta}\ath+\frac{1}{r\sin\theta}\frac{\partial V}{\partial\phi}\aph\]

