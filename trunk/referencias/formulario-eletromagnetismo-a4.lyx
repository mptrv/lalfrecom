\end_layout

\begin_layout Itemize
coordenadas cilíndricas
\end_layout

\begin_layout Standard
\begin_inset Formula \[
\dvg{\vt A}=\frac{1}{\rho}\frac{\partial}{\partial\rho}(\rho A_{\rho})+\frac{1}{\rho}\frac{\partial A_{\phi}}{\partial\phi}+\frac{\partial A_{z}}{\delz}\]

\end_inset


\begin_inset Newpage pagebreak
