\delta=\frac{1}{\alpha}\end{equation}

\end_inset


\begin_inset Newpage pagebreak
\end_inset


\end_layout

\begin_layout Itemize
resistência pelicular:
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{equation}
R_{pelicular}=\frac{1}{\sigma\delta\left(1-e^{-t/\delta}\right)}\end{equation}

\end_inset


\end_layout

\begin_layout Subsection
Teorema de Poynting
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{equation}
\oint\pe{\pvv EH}{\dS}=-\int\pev JE\dv-\frac{\partial}{\partial t}\int\frac{1}{2}\epsilon E^{2}\dv-\frac{\partial}{\partial t}\int\frac{1}{2}\mu H^{2}\dv\end{equation}

\end_inset


\end_layout

