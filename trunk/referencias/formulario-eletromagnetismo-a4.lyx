\end_inset

:
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{equation}
\Gamma=\Gamma_{L}e^{-2\gamma\ell}\end{equation}

\end_inset


\end_layout

\begin_layout Subsection
Taxa de Onda Estacionária de Tensão
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{equation}
ROTE=\frac{1+\left|\Gamma_{L}\right|}{1-\left|\Gamma_{L}\right|}\end{equation}

\end_inset


\end_layout

