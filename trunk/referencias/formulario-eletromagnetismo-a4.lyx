\begin_layout Subsection
Incidência de Um Meio para Outro
\end_layout

\begin_layout Subsubsection
Características da Incidência Normal
\end_layout

\begin_layout Itemize
Coeficiente de reflexão:
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{equation}
\Gamma=\frac{E_{0}^{r}}{E_{0}^{i}}=\frac{\eta_{2}-\eta_{1}}{\eta_{2}+\eta_{1}}\end{equation}

\end_inset


\end_layout

\begin_layout Itemize
coeficiente de transmissão:
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{equation}
\tau=\frac{E_{0}^{t}}{E_{0}^{i}}=\frac{2\eta_{2}}{\eta_{2}+\eta_{1}}\end{equation}

\end_inset


\end_layout

\begin_layout Itemize
relação entre os coeficientes de reflexão e transmissão:
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{equation}
\tau=1+\Gamma\end{equation}

\end_inset


\end_layout

\begin_layout Itemize
taxa de onda estacionária (ROE, COE, TOE, VSWR):
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{equation}
ROE=\frac{E_{max}}{E_{min}}=\frac{V_{max}}{V_{min}}=\frac{1+\left|\Gamma\right|}{1-\left|\Gamma\right|}\end{equation}

\end_inset


\end_layout

\begin_layout Subsubsection
Características da Incidência Oblíqua
\end_layout

\begin_layout Itemize
Coeficiente de reflexão:
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{eqnarray}
\Gamma_{TE} & = & \frac{E_{0}^{r}}{E_{0}^{i}}=\frac{\eta_{2}\cos\theta_{i}-\eta_{1}\cos\theta_{t}}{\eta_{2}\cos\theta_{i}+\eta_{1}\cos\theta_{t}}\end{eqnarray}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula \begin{equation}
\Gamma_{TM}=\frac{E_{0}^{r}}{E_{0}^{i}}=\frac{\eta_{2}\cos\theta_{t}-\eta_{1}\cos\theta_{i}}{\eta_{2}\cos\theta_{t}+\eta_{1}\cos\theta_{i}}\end{equation}

\end_inset


\end_layout

\begin_layout Itemize
coefiente de transmissão:
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{eqnarray}
\tau_{TE} & = & \frac{E_{0}^{t}}{E_{0}^{i}}=\frac{2\eta_{2}\cos\theta_{i}}{\eta_{2}\cos\theta_{i}+\eta_{1}\cos\theta_{t}}\end{eqnarray}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula \begin{equation}
\tau_{TM}=\frac{E_{0}^{t}}{E_{0}^{i}}=\frac{2\eta_{2}\cos\theta_{t}}{\eta_{2}\cos\theta_{t}+\eta_{1}\cos\theta_{i}}\end{equation}

\end_inset


\end_layout

\begin_layout Itemize
Leis de Snell da reflexão e da refração:
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{eqnarray}
\theta_{i} & = & \theta_{r}\\
\frac{\beta_{1}}{\beta_{2}} & = & \frac{\sin\theta_{t}}{\sin\theta_{i}}\end{eqnarray}

\end_inset


\end_layout

\begin_layout Itemize
ângulo de Brewster para polarização TM:
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{equation}
\sin\theta_{BA}=\sqrt{\frac{\beta_{2}^{2}(\eta_{2}^{2}-\eta_{1}^{2})}{\eta_{2}^{2}\beta_{1}^{2}-\eta_{1}^{2}\beta_{2}^{2}}}\end{equation}

\end_inset

 
\end_layout

\begin_layout Section
Linhas de Transmissão
\end_layout

\begin_layout Subsection
Parâmetros Distribuídos
\end_layout

\begin_layout Itemize
Para cabos coaxiais:
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{eqnarray}
R' & = & \frac{1}{2\pi}\left(\frac{1}{a}+\frac{1}{b}\right)\sqrt{\frac{\pi f\mu}{\sigma_{c}}}\\
L' & = & \frac{\mu}{2\pi}\ln\left(\frac{b}{a}\right)\\
G' & = & \frac{2\pi\sigma_{d}}{\ln(b/a)}\\
C' & = & \frac{2\pi\epsilon}{\ln(b/a)}\end{eqnarray}

\end_inset


\end_layout

\begin_layout Itemize
para cabos de condutores gêmeos:
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{eqnarray}
R' & = & \frac{1}{a}\sqrt{\frac{f\mu}{\sigma_{c}}}\\
L' & = & \frac{\mu}{\pi}\cosh^{-1}\left(\frac{d}{2a}\right)\\
G' & = & \frac{\pi\sigma_{d}}{\cosh^{-1}(d/2a)}\\
C' & = & \frac{\pi\epsilon}{\cosh^{-1}(d/2a)}\end{eqnarray}

\end_inset


\end_layout

\begin_layout Subsection
Equações Gerais de Linha de Transmissão
\end_layout

\begin_layout Itemize
No domínio do tempo:
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{eqnarray}
-\frac{\partial v(z,t)}{\partial z} & = & i(z,t)R'+L'\frac{\partial i(z,t)}{\delt}\\
-\frac{\partial i(z,t)}{\delz} & = & v(z,t)G'+C'\frac{\partial v(z,t)}{\delt}\end{eqnarray}

\end_inset


\end_layout

\begin_layout Itemize
no domínio da freqüência (para ondas harmônicas):
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{eqnarray}
\frac{\d V_{s}(z)}{\d z} & = & -(R'+j\omega L')I_{s}(z)\\
\frac{\d I_{s}(z)}{\d z} & = & -(G'+j\omega C')V_{s}(z)\end{eqnarray}

\end_inset


\end_layout

\begin_layout Subsection
Equações de Onda Harmônicas no Tempo
\end_layout

\begin_layout Itemize
No domínio do tempo:
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{eqnarray}
v(z,t) & = & V_{0}^{+}e^{-\alpha z}\cos(\omega t-\beta z)+V_{0}^{-}e^{+\alpha z}\cos(\omega t+\beta z)\qquad\\
i(z,t) & = & I_{0}^{+}e^{-\alpha z}\cos(\omega t-\beta z)+I_{0}^{-}e^{+\alpha z}\cos(\omega t+\beta z)\end{eqnarray}

\end_inset


\end_layout

\begin_layout Itemize
no domínio da freqüência:
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{eqnarray}
V_{s}(z) & = & V_{0}^{+}e^{-\gamma z}+V_{0}^{-}e^{+\gamma z}\\
I_{s}(z) & = & I_{0}^{+}e^{-\gamma z}+I_{0}^{-}e^{+\gamma z}\end{eqnarray}

\end_inset


\end_layout

\begin_layout Standard
ou
\end_layout

\begin_layout Standard

\family roman
\series medium
\shape up
\size normal
\emph off
\bar no
\noun off
\color none
\begin_inset Formula \begin{eqnarray}
V_{s}(z) & = & V_{0}^{+}e^{-\alpha z}e^{-j\beta z}+V_{0}^{-}e^{+\alpha z}e^{+j\beta z}\\
I_{s}(z) & = & I_{0}^{+}e^{-\alpha z}e^{-j\beta z}+I_{0}^{-}e^{+\alpha z}e^{+j\beta z}\end{eqnarray}

\end_inset


\end_layout

\begin_layout Subsection
Constantes de Propagação, Atenuação e de Fase
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{equation}
\gamma=\sqrt{(R'+j\omega L')(G'+j\omega C')}=\alpha+j\beta\end{equation}

\end_inset


\end_layout

\begin_layout Subsection
Impedância Característica
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{equation}
Z_{0}=\frac{V_{0}^{+}}{I_{0}^{+}}=-\frac{V_{0}^{-}}{I_{0}^{-}}=\sqrt{\frac{R'+j\omega L'}{G'+j\omega C'}}\end{equation}

\end_inset


\end_layout

\begin_layout Subsection
Potência
\end_layout

\begin_layout Itemize
Potência média em linha sem perdas:
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{equation}
P_{ave}^{+}(z)=\frac{(V_{0}^{+})^{2}}{2Z_{0}}\end{equation}

\end_inset


\end_layout

\begin_layout Itemize
ganho de potência:
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{equation}
G_{(\deci\bel)}=10\log\left(\frac{P_{out}}{P_{in}}\right)\end{equation}

\end_inset


\end_layout

\begin_layout Itemize
relação entre decibéis e neper:
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{equation}
1\neper=8,686\deci\bel\end{equation}

\end_inset


\end_layout

\begin_layout Subsection
Coeficiente de Reflexão
\end_layout

\begin_layout Itemize
Na carga:
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{equation}
\Gamma_{L}=\frac{V_{0}^{-}}{V_{0}^{+}}=\frac{Z_{L}-Z_{0}}{Z_{L}+Z_{0}}\end{equation}

\end_inset


\end_layout

\begin_layout Itemize
em qualquer ponto:
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{equation}
\Gamma=\frac{V_{0}^{-}e^{+\gamma z}}{V_{0}^{+}e^{-\gamma z}}=\Gamma_{L}e^{+2\gamma z}\end{equation}

\end_inset


\end_layout

\begin_layout Itemize
exemplo 
\begin_inset Formula $\rightarrow$
\end_inset

 
\begin_inset Formula $\Gamma$
\end_inset

 em 
