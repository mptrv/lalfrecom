\begin_layout Itemize
Forma integral:
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{eqnarray}
\oint\pe{\vt D}{\dS} & = & Q_{env}\\
\oint\pe{\vt B}{\dS} & = & 0\\
\oint\pe{\vt E}{\dL} & = & -\frac{\partial}{\partial t}\int\pe{\vt B}{\dS}\\
\oint\pe{\vt H}{\dL} & = & \int\pe{\vt J}{\dS}+\frac{\partial}{\partial t}\int\pe{\vt D}{\dS}\end{eqnarray}

\end_inset


\end_layout

\begin_layout Itemize
forma diferencial:
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{eqnarray}
\dvg{\vt D} & = & \rhv\\
\dvg{\vt B} & = & 0\\
\rot{\vt E} & = & -\frac{\partial\vt B}{\partial t}\\
\rot{\vt H} & = & \vti[\mathrm{c}]J+\frac{\partial\vt D}{\partial t}\end{eqnarray}

\end_inset


\end_layout

\begin_layout Subsection
Representações de Campo Harmônico
\end_layout

\begin_layout Itemize
No domínio do tempo:
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{eqnarray}
\vt E(x,y,z,t) & = & \vt E(x,y,z)\cos(\omega t+\phi)\\
\vt H(x,y,z,t) & = & \vt H(x,y,z)\cos(\omega t+\phi)\end{eqnarray}

\end_inset


\end_layout

\begin_layout Itemize
no domínio da freqüência:
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{eqnarray}
\vti[s]E & = & \vt E(x,y,z)e^{j\phi}\\
\vti[s]H & = & \vt H(x,y,z)e^{j\phi}\end{eqnarray}

\end_inset


\end_layout

\begin_layout Itemize
conversão do domínio da freqüência para o domínio do tempo:
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{eqnarray}
\vt E(x,y,z,t) & = & \re{\vti[s]Ee^{j\omega t}}\\
\vt H(x,y,z,t) & = & \re{\vti[s]He^{j\omega t}}\end{eqnarray}

\end_inset


\end_layout

\begin_layout Subsection
Equações de Maxwell na Forma Fasorial (diferencial)
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{eqnarray}
\dvg{\vti[s]D} & = & \rho_{vs}\\
\dvg{\vti[s]B} & = & 0\\
\rot{\vti[s]E} & = & -j\omega\vti[s]B\\
\rot{\vti[s]H} & = & \vti[s]J+j\omega\vti[s]D\end{eqnarray}

\end_inset


\end_layout

