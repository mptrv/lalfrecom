\end_layout

\begin_layout Itemize
produto Escalar (em coordenadas cartesianas):
\end_layout

\begin_layout Standard
\begin_inset Formula \[
\vt A\centerdot\vt B=\left|\vt A\right|\left|\vt B\right|\cos\theta_{AB}=A_{x}B_{x}+A_{y}B_{y}+A_{z}B_{z}\]

\end_inset


\end_layout

\begin_layout Itemize
operador Nabla:
\end_layout

\begin_layout Standard
\begin_inset Formula \[
\nabla=\frac{\partial}{\delx}\ax+\frac{\partial}{\dely}\ay+\frac{\partial}{\delz}\az\]

