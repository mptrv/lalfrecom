\begin_layout Standard
\begin_inset Formula \begin{eqnarray*}
\vec{A}_{cart} & = & A_{x}\ax+A_{y}\ay+A_{z}\az\\
\vec{A}_{cil} & = & A_{\rho}\arh+A_{\theta}\ath+A_{z}\az\\
\vec{A}_{esf} & = & A_{r}\ar+A_{\theta}\ath+A_{\phi}\aph\end{eqnarray*}

