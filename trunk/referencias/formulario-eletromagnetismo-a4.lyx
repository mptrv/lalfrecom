\end_layout

\end_deeper
\begin_layout Itemize
rotacional:
\end_layout

\begin_deeper
\begin_layout Itemize
coordenadas cartesianas
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{flalign*}
\rot{\vt A} & =\left|\begin{array}{ccc}
\ax & \ay & \az\\
\frac{\partial}{\delx} & \frac{\partial}{\dely} & \frac{\partial}{\delz}\\
A_{x} & A_{y} & A_{z}\end{array}\right|=\\
 & \left(\frac{\partial A_{z}}{\dely}-\frac{\partial A_{y}}{\delz}\right)\ax+\left(\frac{\partial A_{x}}{\delz}-\frac{\partial A_{z}}{\delx}\right)\ay+\left(\frac{\partial A_{y}}{\delx}-\frac{\partial A_{x}}{\dely}\right)\az\end{flalign*}

