\end_inset


\end_layout

\begin_layout Itemize
coordenadas cilíndricas
\end_layout

\begin_layout Standard
\begin_inset Formula \[
\lpl V=\frac{1}{\rho}\frac{\partial}{\partial\rho}\left(\rho\frac{\partial V}{\delrh}\right)+\frac{1}{\rho^{2}}\frac{\partial^{2}V}{\delph^{2}}+\frac{\partial^{2}V}{\delz^{2}}\]

\end_inset


\end_layout

\begin_layout Itemize
coordenadas esféricas
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{alignat*}{1}
 & \lpl V=\\
 & \frac{1}{r^{2}}\frac{\partial}{\delr}\left(r^{2}\frac{\partial V}{\delr}\right)+\frac{1}{r^{2}\sin\theta}\frac{\partial}{\delth}\left(\sin\theta\frac{\partial V}{\delth}\right)+\frac{1}{r^{2}\sin^{2}\theta}\frac{\partial^{2}V}{\delph^{2}}\end{alignat*}

