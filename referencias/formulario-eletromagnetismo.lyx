\begin_layout Subsection
Lei de Faraday
\end_layout

\begin_layout Itemize
Forma geral:
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{equation}
\Vfem=-\frac{\partial\lambda}{\partial t}\end{equation}

\end_inset


\end_layout

\begin_layout Itemize
Para circuito de uma única espira:
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{equation}
\Vfem=\oint\pe{\vt E}{\dL}=-\frac{\partial\Phi}{\partial t}=-\frac{\partial}{\partial t}\int\pe{\vt B}{\dS}\end{equation}

\end_inset


\end_layout

\begin_layout Itemize
Forma diferencial:
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{equation}
\rot{\vt E}=-\frac{\partial\vt B}{\partial t}\end{equation}

\end_inset


\end_layout

\begin_layout Itemize
Para circuito com movimento e campo magnético constante:
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{equation}
\Vfem=\oint\pe{(\pvv uB)}{\dL}\end{equation}

\end_inset


\end_layout

\begin_layout Subsection
Densidade de Corrente de Dispersão (Deslocamento)
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{equation}
\vti[d]J=\frac{\partial\vt D}{\partial t}\end{equation}

\end_inset


\end_layout

\begin_layout Subsection
Equações de Maxwell Gerais
\end_layout

\begin_layout Itemize
Forma integral:
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{eqnarray}
\oint\pe{\vt D}{\dS} & = & Q_{env}\\
\oint\pe{\vt B}{\dS} & = & 0\\
\oint\pe{\vt E}{\dL} & = & -\frac{\partial}{\partial t}\int\pe{\vt B}{\dS}\\
\oint\pe{\vt H}{\dL} & = & \int\pe{\vt J}{\dS}+\frac{\partial}{\partial t}\int\pe{\vt D}{\dS}\end{eqnarray}

\end_inset


\end_layout

\begin_layout Itemize
Forma diferencial:
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{eqnarray}
\dvg{\vt D} & = & \rhv\\
\dvg{\vt B} & = & 0\\
\rot{\vt E} & = & -\frac{\partial\vt B}{\partial t}\\
\rot{\vt H} & = & \vti[\mathrm{c}]J+\frac{\partial\vt D}{\partial t}\end{eqnarray}

\end_inset


\end_layout

\begin_layout Subsection
Representações de Campo Harmônico
\end_layout

\begin_layout Itemize
No domínio do tempo:
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{eqnarray}
\vt E(x,y,z,t) & = & \vt E(x,y,z)\cos(\omega t+\phi)\\
\vt H(x,y,z,t) & = & \vt H(x,y,z)\cos(\omega t+\phi)\end{eqnarray}

\end_inset


\end_layout

\begin_layout Itemize
No domínio da freqüência:
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{eqnarray}
\vti[s]E & = & \vt E(x,y,z)e^{j\phi}\\
\vti[s]H & = & \vt H(x,y,z)e^{j\phi}\end{eqnarray}

\end_inset


\end_layout

\begin_layout Itemize
Conversão do domínio da freqüência para o domínio do tempo:
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{equation}
\vt E(x,y,z,t)=\re{\vti[s]Ee^{j\omega t}}\end{equation}

\end_inset


\end_layout

\begin_layout Subsection
Equações de Maxwell na Forma Fasorial Diferencial
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{eqnarray}
\dvg{\vti[s]D} & = & \rho_{vs}\\
\dvg{\vti[s]B} & = & 0\\
\rot{\vti[s]E} & = & -j\omega\vti[s]B\\
\rot{\vti[s]H} & = & \vti[s]J+j\omega\vti[s]D\end{eqnarray}

\end_inset


\end_layout

