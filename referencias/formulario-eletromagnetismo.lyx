Variação da Densidade de Carga com o Tempo
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{eqnarray}
\rhv & = & \rho_{0}e^{-t/\tau}\end{eqnarray}

\end_inset


\end_layout

