Indutância
\end_layout

\begin_layout Itemize
Definição geral:
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{equation}
L=\frac{\lambda}{I}=N\frac{\Phi_{tot}}{I}\end{equation}

\end_inset


\end_layout

\begin_layout Itemize
Para uma bobina com núcleo:
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{equation}
L=\frac{\mu N^{2}\pi a^{2}}{h}\end{equation}

\end_inset


\end_layout

\begin_layout Itemize
Para um cabo coaxial:
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{equation}
\frac{L}{h}=\frac{\mu}{2\pi}\ln\frac{b}{a}\end{equation}

\end_inset


\end_layout

\begin_layout Subsection
Indutância Mútua
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{equation}
M_{12}=\frac{\lambda_{12}}{I_{1}}=\frac{N_{2}}{I_{1}}\int\pe{\vti[1]B}{\dS_{2}}\end{equation}

\end_inset


\end_layout

\begin_layout Subsection
Energia Magnetostática
\end_layout

\begin_layout Standard
\begin_inset Formula \[
W_{M}=\frac{1}{2}LI^{2}=\frac{1}{2}\int\pev BH\dv\]

