\begin_layout Subsection
Lei de Biot-Savart
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{equation}
d\vti[2]H=\frac{\pv{I\dL_{1}}{\aud}}{4\pi R_{12}^{2}}\end{equation}

\end_inset


\end_layout

\begin_layout Subsection
Campo Magnético Resultante
\end_layout

\begin_layout Itemize
Em termos de elementos diferenciais:
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{equation}
\vt H=\int\frac{\pv{I\dL}{\aR}}{4\pi R^{2}}\end{equation}

\end_inset


\end_layout

\begin_layout Itemize
Em termos de densidades de corrente superficial:
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{equation}
\vt H=\int\frac{\pv{\vt K\dSe}{\aR}}{4\pi R^{2}}\end{equation}

\end_inset


\end_layout

\begin_layout Itemize
Em termos de densidades de corrente volumétrica:
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{equation}
\vt H=\int\frac{\pv{\vt J\dv}{\aR}}{4\pi R^{2}}\end{equation}

\end_inset


\end_layout

\begin_layout Itemize
Devido a linha infinita de corrente:
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{equation}
\vt H=\frac{I\aph}{2\pi\rho}\end{equation}

\end_inset


\end_layout

\begin_layout Itemize
Devido a um solenóide:
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{equation}
\vt H=\frac{NI}{h}\az\end{equation}

\end_inset


\end_layout

\begin_layout Itemize
Devido a uma lâmina infinita de corrente:
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{equation}
\vt H=\frac{1}{2}\pv{\vt K}{\aN}\end{equation}

\end_inset


\end_layout

\begin_layout Subsection
Lei Circuital de Ampère
\end_layout

\begin_layout Itemize
Forma integral:
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{equation}
\oint\pe{\vt H}{\dL}=I_{env}\end{equation}

\end_inset


\end_layout

\begin_layout Itemize
Forma diferencial:
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{equation}
\rot{\vt H}=\vt J\end{equation}

\end_inset


\end_layout

\begin_layout Subsection
Teorema de Stokes
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{equation}
\oint\pe{\vt H}{\dL}=\int\pe{(\rot{\vt H})}{\dS}\end{equation}

\end_inset


\end_layout

\begin_layout Subsection
Densidade de Fluxo Mangnético
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{equation}
\vt B=\mu\vt H\end{equation}

\end_inset


\end_layout

\begin_layout Subsection
Fluxo Magnético
\end_layout

\begin_layout Itemize
Que atravessa uma superfície:
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{equation}
\Phi=\int\pe{\vt B}{\dS}\end{equation}

\end_inset


\end_layout

\begin_layout Itemize
Que atravessa uma superfície fechada:
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{equation}
\oint\pe{\vt B}{\dS}=0\end{equation}

\end_inset


\end_layout

\begin_layout Subsection
Equações de Maxwell para Campos Estáticos
\end_layout

\begin_layout Itemize
\align block
Forma integral:
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{eqnarray*}
\oint\pe{\vt D}{\dS} & = & Q_{env}\\
\oint\pe{\vt B}{\dS} & = & 0\\
\oint\pe{\vt E}{\dL} & = & 0\\
\oint\pe{\vt H}{\dL} & = & I_{env}\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Itemize
Forma diferencial:
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{eqnarray*}
\dvg{\vt D} & = & \rhv\\
\dvg{\vt B} & = & 0\\
\rot{\vt E} & = & 0\\
\rot{\vt H} & = & \vt J\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Subsection
Força
\end_layout

\begin_layout Itemize
Força de Lorentz:
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{equation}
\vt F=q(\vt E+\pvv uB)\end{equation}

\end_inset


\end_layout

\begin_layout Itemize
Força de Campo Magnético sobre Linha de Corrente:
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{equation}
\vti[12]F=\int\pv{I_{2}\dL_{2}}{\vti[1]B}\end{equation}

\end_inset


\end_layout

\begin_layout Subsection
Momento de Dipólo
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{equation}
\vt m=NIS\aN\end{equation}

\end_inset


\end_layout

\begin_layout Subsection
Torque
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{equation}
\vt{\tau}=\pvv mB\end{equation}

\end_inset


\end_layout

\begin_layout Subsection
Condições de Fronteira
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{eqnarray}
\vti[N_{1}]B & = & \vti[N_{2}]B\\
\pv{\aind{21}}{(\vti[1]H-\vti[2]H)} & = & \vt K\end{eqnarray}

\end_inset


\end_layout

